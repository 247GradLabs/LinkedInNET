      <Field Name="data" PropertyName="Data" Type="byte[]"/>
    </ReturnType>   
    <ReturnType Name="UploadAssetResult" ClassName="UploadAssetResponse">
      <Field Name="data" PropertyName="Data" Type="object"/>
