      <Field Name="recipes" PropertyName="Recipes" Type="Recipe" IsCollection="1"/>
      <Field Name="serviceRelationships" PropertyName="ServiceRelationship" Type="ServiceRelationship" IsCollection="1"/>
      <Field Name="status" PropertyName="Status" />
    </ReturnType>
  
    <ReturnType Name="Recipe" ClassName="Recipe">
      <Field Name="recipe" PropertyName="RecipeURN" />
      <Field Name="status" PropertyName="Status" />
    </ReturnType>
  
  </ApiGroup>



