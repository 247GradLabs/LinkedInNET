      <Field Name="firstByte" PropertyName="FirstByte" Type="long"/>
    </ReturnType>

    <ReturnType Name="CompleteMultipartUploadRequest" ClassName="CompleteMultipartUploadRequest">
      <Field Name="completeMultipartUploadRequest" PropertyName="CompleteMultipartUploadRequestData" Type="CompleteMultipartUploadRequestData"/>
    </ReturnType>
    <ReturnType Name="CompleteMultipartUploadRequestData" ClassName="CompleteMultipartUploadRequestData">
      <Field Name="mediaArtifact" PropertyName="MediaArtifact" />
