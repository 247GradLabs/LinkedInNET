    <ReturnType Name="UGCPostvisibility" ClassName="UGCPostvisibility">
      <Field Name="com.linkedin.ugc.MemberNetworkVisibility" PropertyName="comLinkedinUgcMemberNetworkVisibility" />
    </ReturnType>

  </ApiGroup>


  <ApiGroup Name="Asset">
    <ApiMethod MethodName="RegisterUpload" 
               HttpMethod="POST"
               Path="/v2/assets?action=registerUpload"
               PostReturnType="RegisterUploadRequest"
               ReturnType="RegisterUploadResult"
               RequiresUserAuthentication="1"
               UseFieldSelectors="0"
               Title="To upload video."
               Remark="See https://docs.microsoft.com/en-us/linkedin/marketing/integrations/community-management/shares/vector-asset-api#upload-the-asset" />

    <ApiMethod MethodName="UploadAsset" 
               HttpMethod="POST"
               Path="{UploadUrl}"
               PostReturnType="UploadAssetRequest"
               ReturnType="string"
               IsValueType="1"
               IsStreamOctetResultHandeling="1"
               UseFieldSelectors="0"
               Title="To upload video. Gets back the status code of the request."
               Remark="See https://docs.microsoft.com/en-us/linkedin/marketing/integrations/community-management/shares/vector-asset-api#upload-the-asset" />
    
    <ApiMethod MethodName="GetAsset" 
               HttpMethod="GET"
               Path="/v2/assets/{AssetId}"
               ReturnType="AssetResult"
               RequiresUserAuthentication="1"
               UseFieldSelectors="0"
               Title="Depending on content size, it might take a few minutes for the upload to complete."
               Remark="See https://docs.microsoft.com/en-us/linkedin/marketing/integrations/community-management/shares/vector-asset-api#upload-the-asset" />

    <ApiMethod MethodName="CompleteMultiPartUpload"
               HttpMethod="POST"
               Path="/v2/assets?action=completeMultiPartUpload"
               PostReturnType="CompleteMultipartUploadRequest"
               ReturnType="int"
               IsValueType="1"
               IsIntResultHandeling="1"
               RequiresUserAuthentication="1"
               UseFieldSelectors="0"
               Title="To upload video."
               Remark="See https://docs.microsoft.com/en-us/linkedin/marketing/integrations/community-management/shares/vector-asset-api#upload-the-asset" />



    <ReturnType Name="RegisterUploadRequest" ClassName="RegisterUploadRequest">
      <Field Name="registerUploadRequest" PropertyName="RegisterUploadRequestData" Type="RegisterUploadRequestData"/>
    </ReturnType>
    <ReturnType Name="RegisterUploadRequestData" ClassName="RegisterUploadRequestData" >
      <Field Name="supportedUploadMechanism" PropertyName="SupportedUploadMechanism" IsCollection="1"/>
      <Field Name="fileSize" PropertyName="FileSize" Type="long?"/>
      <Field Name="owner" PropertyName="Owner" />
      <Field Name="recipes" PropertyName="Recipes" IsCollection="1"/>
      <Field Name="serviceRelationships" PropertyName="ServiceRelationships" Type="ServiceRelationship" IsCollection="1"/>
    </ReturnType>
    <ReturnType Name="ServiceRelationship" ClassName="ServiceRelationship">
      <Field Name="identifier" PropertyName="Identifier" />
      <Field Name="relationshipType" PropertyName="RelationshipType" />
    </ReturnType>
    
          
    <ReturnType Name="RegisterUploadResult" ClassName="RegisterUploadResult" >
      <Field Name="value" PropertyName="Value" Type="RegisterUploadResultValue"/>
    </ReturnType>
    <ReturnType Name="RegisterUploadResultValue" ClassName="RegisterUploadResultValue" >
      <Field Name="asset" PropertyName="Asset" />
      <Field Name="mediaArtifact" PropertyName="MediaArtifact" />
      <Field Name="uploadMechanism" PropertyName="UploadMechanism" Type="UploadMechanism" />
    </ReturnType>
    <ReturnType Name="UploadMechanism" ClassName="UploadMechanism" >
      <Field Name="com.linkedin.digitalmedia.uploading.MediaUploadHttpRequest" PropertyName="ComLinkedinDigitalmediaUploadingMediaUploadHttpRequest" Type="ComLinkedinDigitalmediaUploadingMediaUploadHttpRequest"/>
      <Field Name="com.linkedin.digitalmedia.uploading.MultipartUpload" PropertyName="ComLinkedinDigitalmediaUploadingMultipartUpload" Type="ComLinkedinDigitalmediaUploadingMultipartUpload"/>
    </ReturnType>
    <ReturnType Name="ComLinkedinDigitalmediaUploadingMediaUploadHttpRequest" ClassName="ComLinkedinDigitalmediaUploadingMediaUploadHttpRequest" >
      <Field Name="headers" PropertyName="Headers" Type="Headers"/>
      <Field Name="uploadUrl" PropertyName="UploadUrl"/>
    </ReturnType>
    <ReturnType Name="ComLinkedinDigitalmediaUploadingMultipartUpload" ClassName="ComLinkedinDigitalmediaUploadingMultipartUpload" >
      <Field Name="metadata" PropertyName="Metadata"/>
      <Field Name="partUploadRequests" PropertyName="PartUploadRequests" Type="PartUploadRequest" IsCollection="1"/>
    </ReturnType>
    <ReturnType Name="PartUploadRequest" ClassName="PartUploadRequest" >
      <Field Name="headers" PropertyName="Headers" Type="Headers"/>
      <Field Name="urlExpiresAt" PropertyName="UrlExpiresAt" Type="long"/>
      <Field Name="byteRange" PropertyName="ByteRange" Type="ByteRange"/>
      <Field Name="url" PropertyName="Url" />
    </ReturnType>
    <ReturnType Name="Headers" ClassName="Headers" >
      <Field Name="Content-Type" PropertyName="ContentType"/>
      <Field Name="Content-Length" PropertyName="ContentLength"/>
      <Field Name="x-amz-server-side-encryption" PropertyName="xAmzServerSideEncryption" />
      <Field Name="x-amz-server-side-encryption-aws-kms-key-id" PropertyName="xAmzServerSideEncryptionAwsKmsKeyId" />
    </ReturnType>
    <ReturnType Name="ByteRange" ClassName="ByteRange" >
