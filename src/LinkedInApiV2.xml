    <ReturnType Name="UGCText" ClassName="UGCText">
      <Field Name="attributes" IsCollection="1" />
      <Field Name="text" />
    </ReturnType>

    <ReturnType Name="LandingPage" ClassName="LandingPage">
      <Field Name="landingPageTitle" />
      <Field Name="landingPageUrl" />
